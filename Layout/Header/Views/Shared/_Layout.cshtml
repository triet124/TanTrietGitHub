<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Header</title>

	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/phosphor-icons@1.4.2/src/css/icons.min.css">

	<link href="~/css/styles.min.css" rel="stylesheet" type="text/css" />
	<link href="~/css/inter.css" rel="stylesheet" type="text/css" />
	<link href="~/css/all.min.css" id="stylesheet" rel="stylesheet" type="text/css" />
	<!-- Core JS files -->
	<script src="~/js/demo_configurator.js"></script>
	<script src="~/js/bootstrap.bundle.min.js"></script>
	<!-- /core JS files -->
	<script src="~/js/d3.min.js"></script>
	<script src="~/js/d3_tooltip.js"></script>

	<script src="~/js/app.js"></script>
	<script src="~/js/dashboard.js"></script>
	<script src="~/js/streamgraph.js"></script>
	<script src="~/js/sparklines.js"></script>
	<script src="~/js/line.js"></script>
	<script src="~/js/areas.js"></script>
	<script src="~/js/donuts.js"></script>
	<script src="~/js/bars.js"></script>
	<script src="~/js/progress.js"></script>
	<script src="~/js/heatmaps.js"></script>
	<script src="~/js/pies.js"></script>
	<script src="~/js/bullets.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/phosphor-icons@1.4.2/src/index.min.js"></script>
</head>

<body>
    @Html.Partial("Header")
    <div>
        <main role="main">
            @RenderBody()
        </main>
    </div>
	@Html.Partial("Footer")

    @RenderSectionAsync("Scripts", required: false)
</body>
</html>
